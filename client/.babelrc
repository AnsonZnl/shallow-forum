{
  "presets": [["next/babel", {
    "transform-runtime": {
      "corejs": 2,
      "helpers": false
    },
    "class-properties": { "loose": true }
  }]],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}